{
  "appName": "Zeeky",
  "version": "0.1.0",
  "logging": {
    "level": "info",
    "file": "logs/zeeky.log"
  },
  "web": {
    "port": 3000,
    "host": "localhost"
  },
  "security": {
    "jwtSecret": "${JWT_SECRET}",
    "apiKey": "${API_KEY}"
  },
  "ai": {
    "defaultProvider": "google",
    "providers": {
      "openai": {
        "apiKey": "${OPENAI_API_KEY}"
      },
      "anthropic": {
        "apiKey": "${ANTHROPIC_API_KEY}"
      },
      "google": {
        "apiKey": "${GEMINI_API_KEY}"
      }
    }
  },
  "plugins": {
    "enabled": [
      "ProductivityPlugin",
      "CreativePlugin",
      "SmartHomePlugin"
    ]
  }
}
