{
  "name": "zeeky-ai-assistant",
  "version": "0.1.0",
  "description": "Cross-platform AI assistant with wake word 'Aye Zeeky' and 10,000+ modular features",
  "main": "dist/index.js",
  "author": "Joachima Ross",
  "license": "MIT",
  "keywords": [
    "ai",
    "assistant",
    "voice",
    "smart-home",
    "automation",
    "enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joachimaross/Joachima-Zeeky.git"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npm run copy-assets",
    "build:watch": "tsc --watch",
    "copy-assets": "copyfiles -u 1 src/**/*.json src/**/*.yml dist/ && copyfiles -u 2 src/web/public/**/* dist/web/public && copyfiles package.json dist/functions",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "security:audit": "npm audit --audit-level high",
    "security:scan": "snyk test",
    "clean": "rimraf dist coverage",
    "docs:generate": "typedoc src --out docs/api",
    "wake-word:train": "ts-node scripts/train-wake-word.ts",
    "plugins:validate": "ts-node scripts/validate-plugins.ts",
    "compliance:check": "ts-node scripts/compliance-check.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@google-cloud/speech": "^7.2.0",
    "@google-cloud/text-to-speech": "^6.3.0",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^24.3.3",
    "axios": "^1.12.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fft-js": "^0.0.12",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.18.1",
    "mqtt": "^5.14.1",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^5.20.2",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "serverless-http": "^4.0.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "wav": "^1.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/aws-lambda": "^8.10.152",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "snyk": "^1.1299.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}