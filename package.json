{
  "name": "zeeky-core",
  "version": "1.0.0",
  "description": "Zeeky - All-in-One AI Assistant with 10,000+ Features",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t zeeky-core .",
    "docker:run": "docker run -p 3000:3000 zeeky-core",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/"
  },
  "keywords": [
    "ai",
    "assistant",
    "voice",
    "automation",
    "smart-home",
    "enterprise",
    "iot"
  ],
  "author": "Zeeky Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@slack/web-api": "^6.9.0",
    "@types/node": "^20.0.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "dotenv": "^16.3.1",
    "dropbox": "^10.34.0",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "modbus-serial": "^8.0.12",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "mqtt": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.5.0",
    "node-cron": "^3.0.2",
    "openai": "^4.0.0",
    "redis": "^4.6.7",
    "sentiment": "^5.0.2",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2",
    "spotify-web-api-node": "^5.0.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.8",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
