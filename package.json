{
  "name": "zeeky-ai-assistant",
  "version": "0.1.0",
  "description": "Cross-platform AI assistant with wake word 'Aye Zeeky' and 10,000+ modular features",
  "main": "dist/index.js",
  "author": "Joachima Ross",
  "license": "MIT",
  "keywords": [
    "ai",
    "assistant",
    "voice",
    "smart-home",
    "automation",
    "enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joachimaross/Joachima-Zeeky.git"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npm run copy-assets",
    "build:watch": "tsc --watch",
    "copy-assets": "copyfiles -u 1 src/**/*.json src/**/*.yml dist/ && copyfiles -u 2 src/web/public/**/* dist/web/public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "security:audit": "npm audit --audit-level high",
    "security:scan": "snyk test",
    "clean": "rimraf dist coverage",
    "docs:generate": "typedoc src --out docs/api",
    "wake-word:train": "ts-node scripts/train-wake-word.ts",
    "plugins:validate": "ts-node scripts/validate-plugins.ts",
    "compliance:check": "ts-node scripts/compliance-check.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.1",
    "@google-cloud/speech": "^6.0.1",
    "@google-cloud/text-to-speech": "^5.0.1",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^20.10.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fft-js": "0.0.12",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongoose": "^8.0.3",
    "mqtt": "^5.3.0",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^4.20.0",
    "pg": "^8.11.3",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "tsyringe": "^4.10.0",
    "uuid": "^9.0.1",
    "wav": "^1.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/bcryptjs": "^2.4.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/module-alias": "^2.0.4",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "snyk": "^1.1248.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
