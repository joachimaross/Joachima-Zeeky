{
  "name": "zeeky-core",
  "version": "1.1.0",
  "description": "Zeeky - All-in-One AI Assistant with 10,000+ Features",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "start:prod": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --ext .ts,.js src/ --fix",
    "format": "prettier --write src/**/*.ts",
    "security-audit": "npm audit --audit-level=high",
    "update-check": "ncu",
    "update": "ncu -u && npm install",
    "docker:build": "docker build -t zeeky/zeeky-core:latest .",
    "docker:run": "docker run -p 3000:3000 zeeky/zeeky-core:latest",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "assistant",
    "voice assistant",
    "automation",
    "smart-home",
    "enterprise",
    "iot",
    "chatbot",
    "machine-learning",
    "nlp",
    "productivity"
  ],
  "author": "Zeeky Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.11.24",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "compromise": "^14.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "googleapis": "^133.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "modbus-serial": "^8.0.17",
    "moment": "^2.30.1",
    "mongoose": "^8.2.0",
    "morgan": "^1.10.0",
    "mqtt": "^5.3.5",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.4",
    "node-cron": "^3.0.3",
    "openai": "^4.28.4",
    "redis": "^4.6.13",
    "sentiment": "^5.0.2",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "tsyringe": "^4.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.17",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeeky/zeeky-core.git"
  },
  "bugs": {
    "url": "https://github.com/zeeky/zeeky-core/issues"
  },
  "homepage": "https://zeeky.ai"
}
