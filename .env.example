# Zeeky Environment Configuration
# Copy this file to .env and update the values

# Application Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# Database Configuration
# PostgreSQL
POSTGRES_URL=postgresql://zeeky:zeeky@localhost:5432/zeeky
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=zeeky
POSTGRES_USER=zeeky
POSTGRES_PASSWORD=zeeky_postgres_password

# MongoDB
MONGODB_URL=mongodb://zeeky:zeeky@localhost:27017/zeeky
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_DB=zeeky
MONGODB_USER=zeeky
MONGODB_PASSWORD=zeeky_mongo_password

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=zeeky_redis_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# AI Services
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_openai_org_id_here

# Anthropic
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here

# Speech Services
# Azure Speech
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=your_azure_speech_region_here

# Google Speech
GOOGLE_SPEECH_API_KEY=your_google_speech_api_key_here

# AWS Polly
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Integration APIs
# Google Services
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here

# Microsoft Graph
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here
MICROSOFT_TENANT_ID=your_microsoft_tenant_id_here

# Slack
SLACK_BOT_TOKEN=your_slack_bot_token_here
SLACK_SIGNING_SECRET=your_slack_signing_secret_here

# Spotify
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Home Automation
# Philips Hue
HUE_BRIDGE_IP=your_hue_bridge_ip_here
HUE_USERNAME=your_hue_username_here

# SmartThings
SMARTTHINGS_TOKEN=your_smartthings_token_here

# Home Assistant
HOME_ASSISTANT_URL=your_home_assistant_url_here
HOME_ASSISTANT_TOKEN=your_home_assistant_token_here

# Security Configuration
ENCRYPTION_KEY=your_encryption_key_here
SALT_ROUNDS=12

# Monitoring and Logging
# Sentry
SENTRY_DSN=your_sentry_dsn_here

# LogRocket
LOGROCKET_APP_ID=your_logrocket_app_id_here

# DataDog
DATADOG_API_KEY=your_datadog_api_key_here

# File Storage
# AWS S3
AWS_S3_BUCKET=your_s3_bucket_name_here
AWS_S3_REGION=us-east-1

# Google Cloud Storage
GOOGLE_CLOUD_STORAGE_BUCKET=your_gcs_bucket_name_here
GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id_here

# Azure Blob Storage
AZURE_STORAGE_ACCOUNT=your_azure_storage_account_here
AZURE_STORAGE_KEY=your_azure_storage_key_here

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password_here

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_here

# Feature Flags
ENABLE_VOICE_COMMANDS=true
ENABLE_VISION_PROCESSING=true
ENABLE_SMART_HOME=true
ENABLE_ENTERPRISE_FEATURES=false

# Development Configuration
DEBUG=zeeky:*
LOG_LEVEL=info
ENABLE_DEBUG_MODE=false

# Performance Configuration
CACHE_TTL=3600
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000

# Compliance Configuration
ENABLE_HIPAA_COMPLIANCE=false
ENABLE_CJIS_COMPLIANCE=false
ENABLE_SOC2_COMPLIANCE=false
ENABLE_GDPR_COMPLIANCE=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
CORS_CREDENTIALS=true

# Session Configuration
SESSION_SECRET=your_session_secret_here
SESSION_MAX_AGE=86400000

# Plugin Configuration
PLUGIN_DIRECTORY=./plugins
ENABLE_PLUGIN_HOT_RELOAD=true
PLUGIN_TIMEOUT=30000

# Mobile SDK Configuration
MOBILE_SDK_VERSION=1.0.0
ENABLE_PUSH_NOTIFICATIONS=true
FCM_SERVER_KEY=your_fcm_server_key_here

# Hardware Hub Configuration
HARDWARE_HUB_ENABLED=false
HARDWARE_HUB_PORT=8080
HARDWARE_HUB_SECRET=your_hardware_hub_secret_here