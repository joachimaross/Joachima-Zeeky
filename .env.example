# Zeeky Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development
HOST=localhost

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/zeeky
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://localhost:5432/zeeky

# Security
JWT_SECRET=your-super-secret-jwt-key-here
BCRYPT_ROUNDS=12
ENCRYPTION_KEY=your-encryption-key-here

# AI Services
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# External Service APIs
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
MICROSOFT_GRAPH_CLIENT_ID=your-ms-client-id
MICROSOFT_GRAPH_CLIENT_SECRET=your-ms-client-secret
GOOGLE_CLOUD_PROJECT_ID=your-gcp-project-id
GOOGLE_CLOUD_SERVICE_ACCOUNT_KEY=path/to/service-account.json

# Smart Home Integration
HOMEBRIDGE_API_KEY=your-homebridge-api-key
MQTT_BROKER_URL=mqtt://localhost:1883
MODBUS_TCP_HOST=localhost
MODBUS_TCP_PORT=502

# Media Services
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret
YOUTUBE_API_KEY=your-youtube-api-key

# File Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=zeeky-storage
DROPBOX_ACCESS_TOKEN=your-dropbox-access-token

# Monitoring & Logging
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn-here
DATADOG_API_KEY=your-datadog-api-key

# Feature Flags
ENABLE_VOICE_COMMANDS=true
ENABLE_SMART_HOME=true
ENABLE_ENTERPRISE_FEATURES=false
ENABLE_BETA_FEATURES=false

# Performance
CACHE_TTL=3600
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000

# Compliance
HIPAA_COMPLIANCE_MODE=false
CJIS_COMPLIANCE_MODE=false
GDPR_COMPLIANCE_MODE=true

# Development
DEBUG=zeeky:*
MOCK_EXTERNAL_APIS=false
SKIP_AUTHENTICATION=false