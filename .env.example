# Zeeky Environment Configuration
# Copy this file to .env and update with your actual values

# Application Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# Database Configuration
MONGODB_URL=mongodb://localhost:27017/zeeky
POSTGRES_URL=postgresql://zeeky:zeeky@localhost:5432/zeeky
REDIS_URL=redis://localhost:6379

# Security Configuration
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
SESSION_SECRET=your_session_secret_here

# AI Service Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Voice Services
SPEECH_TO_TEXT_API_KEY=your_speech_to_text_api_key_here
TEXT_TO_SPEECH_API_KEY=your_text_to_speech_api_key_here

# Integration APIs
HOMEKIT_PASSWORD=your_homekit_password_here
MATTER_COMMISSIONING_CODE=your_matter_commissioning_code_here
CARPLAY_CERTIFICATE_PATH=/path/to/carplay/certificate.pem
ANDROID_AUTO_API_KEY=your_android_auto_api_key_here

# Enterprise Integrations
SALESFORCE_CLIENT_ID=your_salesforce_client_id_here
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret_here
MICROSOFT_GRAPH_CLIENT_ID=your_microsoft_graph_client_id_here
MICROSOFT_GRAPH_CLIENT_SECRET=your_microsoft_graph_client_secret_here
GOOGLE_WORKSPACE_CLIENT_ID=your_google_workspace_client_id_here
GOOGLE_WORKSPACE_CLIENT_SECRET=your_google_workspace_client_secret_here

# Healthcare Integrations
EPIC_CLIENT_ID=your_epic_client_id_here
EPIC_CLIENT_SECRET=your_epic_client_secret_here
CERNER_CLIENT_ID=your_cerner_client_id_here
CERNER_CLIENT_SECRET=your_cerner_client_secret_here

# Media Services
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
YOUTUBE_API_KEY=your_youtube_api_key_here
DROPBOX_ACCESS_TOKEN=your_dropbox_access_token_here

# Monitoring and Analytics
PROMETHEUS_ENDPOINT=http://localhost:9090
GRAFANA_ENDPOINT=http://localhost:3001
ELASTICSEARCH_URL=http://localhost:9200
KIBANA_URL=http://localhost:5601

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE_PATH=./logs/zeeky.log

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_VOICE_FEATURES=true
ENABLE_SMART_HOME=true
ENABLE_ENTERPRISE_FEATURES=false
ENABLE_HEALTHCARE_FEATURES=false

# Performance Configuration
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000
CACHE_TTL=3600
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Security Configuration
CORS_ORIGIN=http://localhost:3000
CORS_CREDENTIALS=true
HELMET_ENABLED=true
RATE_LIMITING_ENABLED=true

# Development Configuration
DEBUG=true
HOT_RELOAD=true
MOCK_EXTERNAL_APIS=false